#Default to test
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/Keys/Robert


#James-Steve Github Account
Host github.com-james
    HostName ssh.github.com
    user git
    IdentityFile ~/.ssh/Keys/JamesSteve
    Port 443

#Robert Github Account
Host github.com-rob
    HostName ssh.github.com
    user git
    IdentityFile ~/.ssh/Keys/Robert
    Port 443


#git@github.com:James-Steve/Linux-Dotfiles-2023.git
